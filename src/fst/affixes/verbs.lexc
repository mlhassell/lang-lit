!! Verb inflection
!  ---------------
!! The Lithuanian language verbs have classes.


LEXICON Verbs_C11
+Pres: Conj_a ;
+Pst: Conj_o ;
+FPst: Freq_Past ;
+Fut: Future ;
+Cond: Subjunctive ;
+Imprt: Imperative ;

LEXICON Verbs_C12
+Pres: Conj_ia ;
+Pst: Conj_e ;
+FPst: Freq_Past ;
+Fut: Future ;
+Cond: Subjunctive ;
+Imprt: Imperative ;

LEXICON Verbs_C2
+Pres: Conj_i ;
+Pst: Conj_ejo ;
+FPst: Freq_Past_e ;
+Fut: Future_e ;
+Cond: Subjunctive_e ;
+Imprt: Imperative_e ;

LEXICON Verbs_C3
+Pres: Conj_o ;
+Pst: Conj_e ;
+FPst: Freq_Past ;
+Fut: Future ;
+Cond: Subjunctive ;
+Imprt: Imperative ;

LEXICON Verbs_C11R
+Pres: Conj_aR ;
+Pst: Conj_oR ;
+FPst: Freq_PastR ;
+Fut: FutureR ;
+Cond: SubjunctiveR ;
+Imprt: ImperativeR ;

LEXICON Verbs_C12R
