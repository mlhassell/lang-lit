+Pst: Conj_eR ;
+FPst: Freq_PastR ;
+Fut: FutureR ;
+Cond: SubjunctiveR ;
+Imprt: ImperativeR ;

LEXICON Verbs_kalb
+Pres: Conj_a ;
+Pst: Conj_ejo ;
+FPst: Freq_Past_e ;
+Fut: Future_e ;
+Cond: Subjunctive_e ;
+Imprt: Imperative_e ;

LEXICON Conj_a
